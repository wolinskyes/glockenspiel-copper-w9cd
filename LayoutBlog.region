<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans&family=Space+Mono&display=swap" rel="stylesheet">

    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="standard_page {squarespace.page-classes}">
    <div class="site-container">
      <header class="site-header">
        <!-- site navigation -->
        <squarespace:navigation navigationId="siteNav" template="site-navigation" />
        {.section website}
          <caption><a href="/" class="site-title-link">{siteTitle}</a></caption>
        {.end}
      </header>

      <main class="content-container max-wrapper" role="main">
        <div id="BlogRoll">
          {squarespace.main-content}
        </div>
      </main>


       <footer class="site-footer">
        <squarespace:block-field id="footerBlocks" columns="12" />
      </footer>
    </div>
    {.section items.0}
    <script>console.log({@|json-pretty})</script>
    {.end}
  </body>
</html>
