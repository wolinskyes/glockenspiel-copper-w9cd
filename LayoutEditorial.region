<!doctype html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <!-- Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans&family=Space+Mono&display=swap" rel="stylesheet">

    <!-- include system scripts, page meta, and header code injection -->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="standard_page {squarespace.page-classes}">
    <div class="site-container">
      <header class="site-header">
        {@|apply site-navigation.block}
      </header>
      <main class="content-container max-wrapper" role="main" data-content-field="main-content">
        <div class="editorial_content_wrap">
          {squarespace.main-content}
        </div>
      </main>
      {@|apply common/footer.block}
    </div>
  </body>
</html>
